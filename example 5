#include <stdio.h>
#include <stdlib.h>

int main(void)
{
    char c;
    printf(" enter the letter :");
    scanf(" %c ", &c);
    if ( isalpha (c))
    printf("%c is an alphabet.", c);
else
    printf("%c is capital letter.", c);
return 0;
}
