#include <stdio.h>
#include <stdlib.h>

int main(void)
{
	char ch;
	printf("enter an alphabet :");
	fflush(stdin); fflush(stdout);
	scanf("%c",&ch);
	if(ch=='a'|| ch=='e'||ch=='i'||ch=='o'||ch=='u')
		printf("it is a vowel");
	else if (ch=='A'|| ch=='E'||ch=='I'||ch=='O'||ch=='U')
		printf("it is a vowel");
	else
		printf("it is a contant");
	return 0;
}
